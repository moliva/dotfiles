# common git config

[url "https://github.com"]
	insteadOf = git://github.com
[color "status"]
	untracked = cyan
[include]
	path = ~/.gitconfig.local
[hub]
        protocol = git
[alias]
        show-files = diff-tree --no-commit-id --name-only -r
	perri-chyck = cherry-pick
	perri-quest = pull-request
[core]
